#: s3backend.cpp:27
#, kde-kuit-format
msgctxt "@info"
msgid ""
"Invalid S3 URI, bucket name is missing from the host.<nl/>A valid S3 URI "
"must be written in the form: <link>s3://bucket/key</link>"
msgstr ""
"Nederīgs S3 URI, resursdatorā trūkst groza nosaukuma.<nl/>Derīgam S3 URI "
"jābūt rakstītam formā: <link>s3://grozs/atslēga</link>"

#: s3backend.cpp:54
#, kde-kuit-format
msgctxt "@info"
msgid "Could not find S3 buckets, please check your AWS configuration."
msgstr "Neizdevās atrast S3 grozus. Pārbaudiet AWS konfigurāciju."

#: s3backend.cpp:313
#, kde-kuit-format
msgctxt "@info"
msgid "Could not copy <link>%1</link> to <link>%2</link>"
msgstr "Neizdevās pārkopēt <link>%1</link> uz <link>%2</link>"
